# BigQuery Configuration (使用 thrasio-dev-data-wh-7ee095 项目)
GCP_PROJECT_ID="YOUR_GCP_PROJECT_ID"
GCP_REGION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/bigquery-service-account.json

# BigQuery specific settings
BIGQUERY_DATASET=analytics
BIGQUERY_LOCATION=US
BIGQUERY_TIMEOUT=60
BIGQUERY_MAX_RESULTS=10000

# LLM Configuration (使用 thrasio-dev-ai-agent 项目)
LLM_PROJECT_ID="YOUR_GCP_PROJECT_ID"
LLM_PROVIDER=vertex-ai
LLM_MODEL_NAME=gemini-2.5-flash
LLM_TEMPERATURE=0.1
LLM_TIMEOUT=30
VERTEX_AI_LOCATION=us-central1
LLM_GOOGLE_APPLICATION_CREDENTIALS=/path/to/llm-service-account.json

# Other configurations
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
LOG_FORMAT=json

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Worker Configuration
WORKER_MAX_CONCURRENT_TASKS=10
WORKER_TASK_TIMEOUT=300
WORKER_RETRY_ATTEMPTS=3
WORKER_RETRY_DELAY=5
WORKER_QUEUE_NAME=data_analysis
WORKER_DEAD_LETTER_QUEUE=data_analysis_dlq
WORKFLOW_TIMEOUT=600
MAX_WORKFLOW_RETRIES=2